@model IList<YangKai.BlogEngine.Domain.Post>
<!doctype html>
<html>
    <head>
        <meta charset="utf-8" />
        <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
        <title>@Html.Raw(YangKai.BlogEngine.Common.Config.Literal.SITE_NAME)</title>
        <meta name="viewport" content="width=device-width" />
        <meta name="fragment" content="!">
    </head>
    <body>
        <h1>iShare</h1>
        <ul>
            @foreach (var item in Model)
            {
            <li><a href="/post/@item.Url">@item.Title</a></li>
            }
        </ul>
    </body>
</html>